{
  "name": "message-servive",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "watch": "ts-node-dev --respawn index.dev.ts",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "db:migrate": "ts-node ./node_modules/.bin/typeorm migration:run -d ormconfig.ts",
    "db:migrate:down": "ts-node ./node_modules/.bin/typeorm migration:revert -d ormconfig.ts",
    "db:migration": "ts-node ./node_modules/.bin/typeorm migration:create"
  },
  "lint-staged": {
    "src/**/*.{js,ts,json}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/config": "^0.0.41",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.0.0",
    "@types/prettier": "^2.6.3",
    "@types/uuid": "^8.3.4",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "module-alias": "^2.2.2",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.13.2",
    "confg": "^0.0.5",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "http-errors": "^2.0.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.7",
    "uuid": "^8.3.2",
    "winston": "^3.8.0"
  },
  "_moduleAliases": {
    "@root": "./src"
  }
}
